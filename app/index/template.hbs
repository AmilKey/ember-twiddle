Index

{{#each files as |file|}}
  <h2>{{file.name}}</h2>

  <code>
    <pre>{{ivy-codemirror
        value         = file.content
        mode          = 'javascript'
        theme         = 'solarized'
        lineNumbers   = true
    }}</pre>
  </code>

  <h2>Compiled</h2>

  <code>
    <pre>{{file.compiled}}</pre>
  </code>
{{/each}}